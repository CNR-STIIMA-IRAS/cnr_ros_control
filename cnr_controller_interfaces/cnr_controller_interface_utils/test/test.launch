</rosparam>
</group>

<group ns="only_file_streamer">
<rosparam>
  appenders: [file]
  levels: [debug]
  file_name: "ciao"
</rosparam>
</group>


<test test-name="cnr_controller_interface_utils_test" pkg="cnr_controller_interface_utils" type="cnr_controller_interface_utils_test">
</test>


</launch>



